   "source": [
    "import hashlib\n",
    "def hachage(texte):\n",
    "    hash1 = hashlib.sha256()\n",
    "    hash1.update(texte.encode('utf-8'))\n",
    "    return hash1.hexdigest()\n",
    "texte =input(\"entrez le mot que vous désirez cripter \")\n",
    "texte_hashe = hachage(texte)\n",
    "print(\"le texte hashé correspond à : \",texte_hashe)"
   ]
